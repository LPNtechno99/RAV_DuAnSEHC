<Window x:Class="AssyChargeSEHC.wdLoadingCurrent"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssyChargeSEHC"
        mc:Ignorable="d"
        Title="wdLoadingCurrent" Height="100" Width="600" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        Loaded="Window_Loaded" WindowStyle="None" Closing="Window_Closing">
    <StackPanel Orientation="Vertical">
        <TextBlock Margin="5 10 5 0" Text="Setting Current..." FontSize="14" FontWeight="Bold"/>
        <ProgressBar Margin="5 20 5 0" Height="30" Minimum="0" Maximum="100" IsIndeterminate="True"/>
    </StackPanel>
</Window>
