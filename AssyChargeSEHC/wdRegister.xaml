<Window x:Class="AssyChargeSEHC.wdRegister"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssyChargeSEHC"
        mc:Ignorable="d"
        Title="wdRegister" Height="120" Width="780" WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize" Loaded="Window_Loaded">
    <StackPanel Orientation="Horizontal" Margin="5">
        <TextBlock Text="Key Active:" FontSize="18" FontWeight="Bold" VerticalAlignment="Center"
                   Margin="10 0 0 0"/>
        <TextBox Margin="10 0 0 0" Height="35" Width="550" FontSize="18" Name="txtKeyActive"
                 FontWeight="Bold" TextAlignment="Right" Padding="0 0 2 0"/>
        <Button Margin="20 0 0 0" Height="35" Width="60" Content="OK" FontWeight="Bold"
                FontSize="18" Name="btnOK" Click="btnOK_Click"/>
    </StackPanel>
</Window>
