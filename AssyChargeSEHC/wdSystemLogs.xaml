<Window x:Class="AssyChargeSEHC.wdSystemLogs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssyChargeSEHC"
        mc:Ignorable="d"
        Title="wdSystemLogs" Height="650" Width="850" ResizeMode="NoResize"
        Loaded="Window_Loaded" WindowStartupLocation="CenterScreen">
    <DockPanel Margin="5">
        <Label DockPanel.Dock="Top" Content="SystemLogs" FontSize="20" FontWeight="Bold"/>
        <ListBox x:Name="lstLogs" FontSize="14" FontStyle="Italic"
                 Margin="0 10 0 0" Padding="5"/>
    </DockPanel>
</Window>
