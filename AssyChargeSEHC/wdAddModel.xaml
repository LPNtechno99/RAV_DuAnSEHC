<Window x:Class="AssyChargeSEHC.wdAddModel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssyChargeSEHC"
        mc:Ignorable="d"
        Title="wdAddModel" Height="250" Width="700" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="5*"/>
        </Grid.RowDefinitions>
        <Label Content="Add Model" VerticalContentAlignment="Center" FontSize="26" FontWeight="Bold"
               Background="#FF1A3CD6" Foreground="White"/>
        <TextBox Grid.Row="1" Name="tbAddModel" Margin="10" FontSize="30" FontWeight="Bold" KeyUp="TextBox_KeyUp"
                 TextWrapping="Wrap"/>
    </Grid>
</Window>
